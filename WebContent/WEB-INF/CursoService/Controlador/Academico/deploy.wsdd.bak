<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CursoService WSDL service -->

  <service name="Curso" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Academico.Controlador"/>
      <parameter name="wsdlServiceElement" value="CursoService"/>
      <parameter name="schemaQualified" value="http://Academico.Controlador"/>
      <parameter name="wsdlServicePort" value="Curso"/>
      <parameter name="className" value="Controlador.Academico.CursoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Curso"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="calcularCantCursos" qname="operNS:calcularCantCursos" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:calcularCantCursosReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:rut" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="actualizarCambios" qname="operNS:actualizarCambios" xmlns:operNS="http://Academico.Controlador" soapAction="" >
        <parameter qname="pns:rut" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="inscribirEstudiantes" qname="operNS:inscribirEstudiantes" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:inscribirEstudiantesReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idCurso" xmlns:pns="http://Academico.Controlador" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutEstudiante" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutProf" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="desactivarCurso" qname="operNS:desactivarCurso" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:desactivarCursoReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://Academico.Controlador" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutDirector" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="crearCurso" qname="operNS:crearCurso" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:crearCursoReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nombreCurso" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutJefeAdm" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getNombreCurso" qname="operNS:getNombreCurso" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:getNombreCursoReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="asignarProfesor" qname="operNS:asignarProfesor" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:asignarProfesorReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://Academico.Controlador" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutProfe" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutJefeAdm" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="setNombre" qname="operNS:setNombre" xmlns:operNS="http://Academico.Controlador" soapAction="" >
        <parameter qname="pns:nombre" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="bucarCursos" qname="operNS:bucarCursos" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:bucarCursosReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:ArrayOf_xsd_string" xmlns:rtns="http://Academico.Controlador" soapAction="" >
        <parameter qname="pns:rut" xmlns:pns="http://Academico.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="buscarCurso" qname="operNS:buscarCurso" xmlns:operNS="http://Academico.Controlador" returnQName="retNS:buscarCursoReturn" xmlns:retNS="http://Academico.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idCurso" xmlns:pns="http://Academico.Controlador" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="asignarProfesor buscarCurso calcularCantCursos actualizarCambios crearCurso inscribirEstudiantes setNombre desactivarCurso getNombreCurso bucarCursos"/>

      <arrayMapping
        xmlns:ns="http://Academico.Controlador"
        qname="ns:ArrayOf_xsd_string"
        type="java:java.lang.String[]"
        innerType="cmp-ns:string" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
