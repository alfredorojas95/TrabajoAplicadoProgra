<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MatriculaService WSDL service -->

  <service name="Matricula" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Finanza.Controlador"/>
      <parameter name="wsdlServiceElement" value="MatriculaService"/>
      <parameter name="schemaQualified" value="http://Finanza.Controlador"/>
      <parameter name="wsdlServicePort" value="Matricula"/>
      <parameter name="className" value="Controlador.Finanza.MatriculaSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Matricula"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="pagarMatricula" qname="operNS:pagarMatricula" xmlns:operNS="http://Finanza.Controlador" returnQName="retNS:pagarMatriculaReturn" xmlns:retNS="http://Finanza.Controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:rutEstudiante" xmlns:pns="http://Finanza.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:rutSecretaria" xmlns:pns="http://Finanza.Controlador" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="obtenerListMorososMatricula" qname="operNS:obtenerListMorososMatricula" xmlns:operNS="http://Finanza.Controlador" returnQName="retNS:obtenerListMorososMatriculaReturn" xmlns:retNS="http://Finanza.Controlador" returnType="rtns:ArrayOf_xsd_string" xmlns:rtns="http://Finanza.Controlador" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="obtenerListMorososMatricula pagarMatricula"/>

      <arrayMapping
        xmlns:ns="http://Finanza.Controlador"
        qname="ns:ArrayOf_xsd_string"
        type="java:java.lang.String[]"
        innerType="cmp-ns:string" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
