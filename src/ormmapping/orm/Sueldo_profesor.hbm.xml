<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Sueldo_profesor" table="sueldo_profesor" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Sueldo" column="sueldoid" class="orm.Sueldo">
			</key-many-to-one>
			<key-many-to-one name="ORM_Profesor" column="profesorid" class="orm.Profesor">
			</key-many-to-one>
		</composite-id>
		<many-to-one name="jefeadministracion" cascade="lock" column="jefeadministracionid" class="orm.Jefeadministracion" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
