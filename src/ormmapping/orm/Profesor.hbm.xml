<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Profesor" table="profesor" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="persona" column="personaid" class="orm.Persona" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<set name="ORM_Curso_profesor" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="profesorid" not-null="true"/>
			<one-to-many class="orm.Curso_profesor"/>
		</set>
		<set name="ORM_Sueldo" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="profesorid" not-null="true"/>
			<one-to-many class="orm.Sueldo"/>
		</set>
	</class>
</hibernate-mapping>
